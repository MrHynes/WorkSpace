package Chapter01;

public class main {
	public static void main(String[] args) {
		Search search=new Search();
		int a[]={1,3,4,6,7};
		int i=search.linearSearch(a, 4);
		System.out.println(i);
		int j=search.binarySearch(a, 4);
		System.out.println(j);
		Sort sort=new Sort();
		int b[]={2,3,5,8,9,10};
		System.out.println(sort.Merge(a, b));

	}
}
