package pers.qiqcheng.spring.demo05;

public class Person {
	private String name;
	private float score;
	public void setName(String name) {
		this.name = name;
	}
	public void setScore(float score) {
		this.score = score;
	}
	@Override
	public String toString() {
		return "Person [name=" + name + ", score=" + score + "]";
	}
}
