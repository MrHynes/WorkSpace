package pers.qiqcheng.exp01_01;

import java.util.Scanner;

public class main {
	
	public static void main(String[] args) {
		Scanner scanner=null;
		int n=scanner.nextInt();
		int m;
		for(int i=0;i<n;i++){
			m=scanner.nextInt();
			int a[]=new int[m];
			for(int j=0;j<m;j++){
				a[j]=scanner.nextInt();
			}
		}
	}
}
