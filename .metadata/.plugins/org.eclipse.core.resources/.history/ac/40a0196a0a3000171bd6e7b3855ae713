package pers.qiqcheng.spring.demo09;

public class main {
	public static void main(String[] args) {
		ProductDao productDao=new ProductDao();
		productDao.add();
		productDao.update();
		/*userDao.add();
		userDao.update();*/
		UserDao proxy=new JDKProxy(userDao).createProxy();
		proxy.add();
		proxy.update();
	}

}
