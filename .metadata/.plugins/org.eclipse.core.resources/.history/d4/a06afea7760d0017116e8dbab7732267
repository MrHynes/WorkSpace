package Chapter05.execise;

public class FinfTwoNumberEqualsX5_33 {

	public boolean findTwoNumber(int a[],int low,int high,int x){
		boolean flag=false;
		if(low<high&&!flag){
			if(a[low]+a[high]>x){
				high--;
			}
			if(a[low]+a[high]<x){
				low++;
			}
			else {
				flag=true;
			}
		}
		System.err.println(a[low]+"+"+a[high]+"="+x);
		return flag;
	}
	public static void main(String[] args) {
		
	}
}
