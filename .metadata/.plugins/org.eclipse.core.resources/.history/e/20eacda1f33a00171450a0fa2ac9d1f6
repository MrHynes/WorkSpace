
public class HappynUmber {

	public static boolean getHappyNumber(int number){
		boolean flag=false;
		if(number==1) flag= true;
		else{
			int numA=number/10;
			int numB=number%10;
			flag=getHappyNumber();
		}
		return flag;
	}
	public static void main(String[] args) {
		System.out.println(getHappyNumber(10));
	}
}
