
public class main {
	public static int  binarysearch(int a[],int low ,int high,int x){
		int mid;
		int k=-1;
		while(low<=high){
			mid=(low+high)/2;
			if(a[mid]==x){
				k=mid;
				break;
			}
			else if(a[mid]>x){
				high=mid-1;
			}else {
				low=mid+1;
			}
		}
		return k;
	}
	public static void main(String[] args) {
		int a[]={1,3,4,5,6};
		System.out.println(binarysearch(a, 0, a.length-1, 3));
	}

}
