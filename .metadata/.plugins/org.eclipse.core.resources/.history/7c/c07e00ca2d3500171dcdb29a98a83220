package Chapter07;

import Chapter01.main;

/**
 * 矩阵链乘问题
 * @author Buer
 *
 */
//r[i]数组，表示矩阵的阶，如A的矩阵为2*3，B的矩阵为3*4，那么数组r[]={2,3,4};
public class Matchain {
	public static void mtachain(int n,int r[]){
		int C[][]=new int[n+1][n+1]; 
		for(int i=1;i<n;i++){
			C[i][i]=0;
		}
		int j;
		for(int d=1;d<n;d++){
			for(int i=1;i<=n-d;i++){
				j=i+d;
				C[i][j]=Integer.MAX_VALUE;//
				for(int k=){
					C[i][j]=main();
				}
			}
		}
	}

}
