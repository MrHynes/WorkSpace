<script type="text/javascript">
	$(function() {
		$("#click2").click(function () {
			var value=$("#check").val;
			for(var i=0;i<value.length;i++){
				alert(value[i]);
			}
		});
		$("#click").click(function () {
			
			bootbox.prompt({
			    title: "This is a prompt with a set of checkbox inputs!",
			    inputType: 'checkbox',
			    inputOptions: [
			        {
			            text: 'Choice One',
			            value: '1',
			        },
			        {
			            text: 'Choice Two',
			            value: '2',
			        },
			        {
			            text: 'Choice Three',
			            value: '3',
			        }
			    ],
			    callback: function (result) {
			        if(result){
			        	location.href = "/ECProject/getinfo";
			        }else{
			        	return;
			        }
			    }
			});
		});
	});

</script>
