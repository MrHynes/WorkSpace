
public class NumOfPrim {
	public static boolean isPrim(int a)
	{
	  boolean flag=true;
	  int i;
	  for(i=0;i<a/2;i++)
	  {
	    if(a%i==0)
	    {
	      flag=false;
	      break;
	    }
	  }
	  return flag;
	}
	int main()
	{
	  int n;
	  int sum=0;
	  scanf("%d",&n);
	  int i;
	  for(i=2;i<n;i++)
	  {
	    if(isPrim(i)&&isPrim(n-i))
	    {
	    	sum++;
	    }
	  }
	  printf("%d",sum);
	}
	  
}
